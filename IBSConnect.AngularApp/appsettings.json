{
  "NLog": {
    "targets": {
      "console": {
        "type": "Console"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "console"
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "CutoffHours": 8,
    "WebTokenExpiry": {
      "Hours": 12
    },
    "AppTokenExpiry": {
      "Hours": 12
    },
    "AdminTokenExpiry": {
      "Hours": 2
    }
  }

}
